{
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "moduleResolution": "node",
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "outDir": "lib",
    "skipLibCheck": true,
    "rootDir": "."
  }
}
